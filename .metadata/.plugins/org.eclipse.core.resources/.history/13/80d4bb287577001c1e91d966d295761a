package ArrayAddition;

public class Additions {
	
	 public static void main(String [] args)
	 
	 
	 {
		 int [] [] a= { 
				 {1, 1, 1,},
				 {1, 1, 1,},
				 {1, 1, 1,}
	                	 };
		 int [] [] b= {
				 {2, 2, 2,},
				 {2, 2, 2,},
				 {2, 2, 2,}
		                  };
		 
		    System.out.println("== Matrix A ==");
		         for (int i=0;i<3;i++) {
		         	for (int j=0;j<3;j++) {
		    		    System.out.print(a[i][j]+" ");
		         }
		     
		         }
		               System.out.println();
	 }
}
