package FirstandLast;

public class FirstandLastArrayDemo {
	
	  public static void main(String[] args)
	  
	  { 
		  int a[] =new int [] {1,2,3,4,5};
		  
		 
		  int size =a.length;
		  
		     System.out.println(" First element an array is" +a[0]);
		     
		     System.out.println("Last element an array is" +a[size-1]);
	  }

}
